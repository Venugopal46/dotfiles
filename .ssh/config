Host *
    UseKeychain yes

# personalGithub
Host personalGithub
   HostName github.com
   User git
   IdentityFile ~/.ssh/id_rsa_personal
   IdentitiesOnly yes

# personalDigitalOcean
Host personalDigitalOcean
   HostName hostIP
   User root
   IdentityFile ~/.ssh/id_rsa_digitalocean
   IdentitiesOnly yes

# workGitHub
Host workGitHub
   HostName url1
   User git
   IdentityFile ~/.ssh/id_rsa_work
   IdentitiesOnly yes

# workBitBucket
Host workBitBucket
   HostName url2
   User git
   IdentityFile ~/.ssh/id_rsa_work
   IdentitiesOnly yes

 # workVisualStudio
Host workVisualStudio
   HostName url3
   User walmart
   IdentityFile ~/.ssh/id_rsa_vs
   IdentitiesOnly yes
